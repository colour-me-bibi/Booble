{
    "name": "Booble",
    "version": "0.2",
    "description": "Upgrades the Google search page",
    "manifest_version": 2,
    "web_accessible_resources": ["*.png"],
    "icons": {
        "128": "images/logo_128.png"
    },
    "content_scripts": [ {
            "js": ["content.js"],
            "matches": ["https://www.google.com/"]
        } ],
    "browser_action": {
        "default_icon": "images/logo_128.png"
    }
}
