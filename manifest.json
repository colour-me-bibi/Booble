{
    "name": "Booble",
    "version": "0.1",
    "description": "Upgrades the Google seaerch page",
    "manifest_version": 2,
    "web_accessible_resources": ["*.png"],
    "icons": {
        "128": "images/logo_128.png"
    },
    "content_scripts": [ {
            "js": ["content.js"],
            "matches": ["https://www.google.com/"],
            "run_at": "document_end"
        } ],
    "browser_action": {
        "default_icon": "images/logo_128.png"
    }
}
